{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "excerpt": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "publish_date": {
      "type": "datetime"
    },
    "feature_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "published": {
      "type": "boolean"
    },
    "author": {
      "plugin": "admin",
      "model": "user"
    },
    "tags": {
      "model": "category",
      "collection": "tag"
    },
    "categories": {
      "model": "category",
      "collection": "category"
    },
    "original_date": {
      "type": "datetime"
    },
    "comments": {
      "model": "comment",
      "collection": "comment"
    },
    "wp_id": {
      "type": "integer",
      "unique": true
    }
  }
}
